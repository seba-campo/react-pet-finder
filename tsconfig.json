{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES5",
    "outDir": "dist",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./src"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "customs.d.ts"],
  "paths": {
      "pages/*": ["pages/*"],
      "components/*": ["components/*"],
      "@/*": ["*"]
    }
}